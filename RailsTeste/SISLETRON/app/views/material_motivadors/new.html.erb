<%= tinymce_assets %>

<%= tinymce %>

<%= form_for(@material_motivador) do |f| %>
  <div class="field">
    <%= f.label :material_motivador %><br>
    <%= f.text_area :titulo %><br>
    <%= f.text_area :conteudo, :class => "tinymce", :rows => 20, :cols => 120 %>
  </div>
  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>
